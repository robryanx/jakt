function pass_slice(slice: [i64..]) -> [i64..] {
    return slice
}

function pass_array(arr: Array<i64>) -> Array<i64> {
    return arr
}

// Array<i64> -> [i64]
// ArraySlice<i64> -> [i64..]

function main() {
    mut arr = [1, 2, 3, 4]
    let slice = arr[..]
    arr.push(5)

