/// Expect:
/// - output: "A = 1, B = 2, C = 69\nA = 0, B = 1, C = 2\n"

enum CuteLittleEnum: u64 {
    A = 1
    B
    C = 69
}

enum CuterLittlerEnum: u32 {
    A
    B
    C
}

function main() {
    println("A = {}, B = {}, C = {}",
        CuteLittleEnum::A,
        CuteLittleEnum::B,
        CuteLittleEnum::C)

    println("A = {}, B = {}, C = {}",
        CuterLittlerEnum::A,
        CuterLittlerEnum::B,
        CuterLittlerEnum::C)
}
